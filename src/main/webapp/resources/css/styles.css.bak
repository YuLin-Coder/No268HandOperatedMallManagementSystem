	min-height: 1.75rem;
	margin: 0;
	font-size: 1.125rem;
	font-weight: 600;
	line-height: 1.75;
}

.bg-body-light {
	background-color: #f9fafc!important;
	padding-bottom: 20px;
}

.block.block-bordered {
	border: 1px solid #e6ebf4;
	box-shadow: none;
}

.pricing-content {
    min-height: 280px;
}
.block {
	display: block;
}

.fa-check:before {
