}
.block-header {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	padding: .75rem 1.25rem;
	transition: opacity .25s ease-out;
}

.block-content {
	transition: opacity .25s ease-out;
	width: 100%;
	margin: 0 auto;
	padding: 1.25rem 1.25rem 1px;
	overflow-x: visible;
}
.no-margin-padding{
